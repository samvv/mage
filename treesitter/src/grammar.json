{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mage",
  "rules": {
    "grammar": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "module_element"
      }
    },
    "module_element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "rule"
        },
        {
          "type": "SYMBOL",
          "name": "module"
        }
      ]
    },
    "module": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "mod"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "module_element"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "integer": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "rule": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "pub"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "token"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "expr"
        }
      ]
    },
    "_prim_expr": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "label",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "identifier"
                  },
                  {
                    "type": "STRING",
                    "value": ":"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "lit_expr"
            },
            {
              "type": "SYMBOL",
              "name": "ref_expr"
            },
            {
              "type": "SYMBOL",
              "name": "opt_expr"
            },
            {
              "type": "SYMBOL",
              "name": "many_expr"
            },
            {
              "type": "SYMBOL",
              "name": "some_expr"
            },
            {
              "type": "SYMBOL",
              "name": "repeat_expr"
            },
            {
              "type": "SYMBOL",
              "name": "nest_expr"
            }
          ]
        }
      ]
    },
    "choice_expr": {
      "type": "PREC_LEFT",
      "value": 4,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "expr"
          },
          {
            "type": "STRING",
            "value": "|"
          },
          {
            "type": "SYMBOL",
            "name": "expr"
          }
        ]
      }
    },
    "expr": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "choice_expr"
        },
        {
          "type": "SYMBOL",
          "name": "seq_expr"
        },
        {
          "type": "SYMBOL",
          "name": "_prim_expr"
        }
      ]
    },
    "escape_sequence": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\\"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "u[a-fA-F\\d]{4}"
                },
                {
                  "type": "PATTERN",
                  "value": "U[a-fA-F\\d]{8}"
                },
                {
                  "type": "PATTERN",
                  "value": "x[a-fA-F\\d]{2}"
                },
                {
                  "type": "PATTERN",
                  "value": "\\d{1,3}"
                },
                {
                  "type": "PATTERN",
                  "value": "\\r?\\n"
                },
                {
                  "type": "PATTERN",
                  "value": "['\"abfrntv\\\\]"
                },
                {
                  "type": "PATTERN",
                  "value": "N\\{[^}]+\\}"
                }
              ]
            }
          ]
        }
      }
    },
    "lit_expr": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "PREC",
                  "value": 1,
                  "content": {
                    "type": "PATTERN",
                    "value": "[^\\\\\"\\n]+"
                  }
                }
              },
              {
                "type": "SYMBOL",
                "name": "escape_sequence"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "ref_expr": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "seq_expr": {
      "type": "PREC_LEFT",
      "value": 5,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_prim_expr"
          },
          {
            "type": "SYMBOL",
            "name": "_prim_expr"
          }
        ]
      }
    },
    "opt_expr": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_prim_expr"
        },
        {
          "type": "STRING",
          "value": "?"
        }
      ]
    },
    "some_expr": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_prim_expr"
        },
        {
          "type": "STRING",
          "value": "+"
        }
      ]
    },
    "many_expr": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_prim_expr"
        },
        {
          "type": "STRING",
          "value": "*"
        }
      ]
    },
    "repeat_expr": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_prim_expr"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "integer"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ","
                },
                {
                  "type": "SYMBOL",
                  "name": "integer"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "nest_expr": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "expr"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [
    [
      "_prim_expr"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}