{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "mage",
      "camelcase": "Mage",
      "title": "Mage",
      "scope": "source.mage",
      "file-types": [
        "mage"
      ],
      "injection-regex": "^mage$",
      "class-name": "TreeSitterMage"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A lexer/parser generator for a growing number of languages",
    "authors": [
      {
        "name": "Sam Vervaeck",
        "email": "samvv@pm.me",
        "url": "https://samvervaeck.be/"
      }
    ],
    "links": {
      "repository": "https://github.com/samvv/mage"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}